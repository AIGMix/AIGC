                "-o",
                "${workspaceRoot}/main.exe",
                "-I${workspaceRoot}/AIGC/include",
                // "-I${workspaceRoot}/Lanzouyun",
                "-std=c++17",
                "-lws2_32",  
                
            ],
            "problemMatcher": {
                "owner": "cpp",
                "fileLocation": [
                    "relative",
                    "${workspaceRoot}"
                ],
                "pattern": {
                    "regexp": "^(.*):(\\d+):(\\d+):\\s+(warning|error):\\s+(.*)$",
                    "file": 1,
