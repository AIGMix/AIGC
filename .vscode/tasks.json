{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "aigc",
            "command": "g++",
            "args": [
                "-g",
                "${workspaceRoot}/Main.cpp",
                "-o",
                "${workspaceRoot}/main.exe",
                "-I${workspaceRoot}/AIGC/include",
                "-std=c++17",
                "-lws2_32",
            ],
            "problemMatcher": {
                "owner": "cpp",
                "fileLocation": [
                    "relative",
                    "${workspaceRoot}"
                ],
                "pattern": {
                    "regexp": "^(.*):(\\d+):(\\d+):\\s+(warning|error):\\s+(.*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "message": 5
                }
            },
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "tidal",
            "command": "g++.exe",
            "args": [
                "-g",
                "${workspaceRoot}/Tidal/*.cpp",
                "-o",
                "${workspaceRoot}/main.exe",
                "-I${workspaceRoot}/AIGC/include",
                "-I${workspaceRoot}/Tidal",
                "-std=c++17",
                "-lws2_32",  
                
            ],
            "problemMatcher": {
                "owner": "cpp",
                "fileLocation": [
                    "relative",
                    "${workspaceRoot}"
                ],
                "pattern": {
                    "regexp": "^(.*):(\\d+):(\\d+):\\s+(warning|error):\\s+(.*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "message": 5
                }
            },
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
